{% Display Resources %}
{% Display FacebookPagePlugin %}
<div id="wrapper">
    <div class="sticky-wrapper">
        <header class="header sticky-header">
            <div class="header-bottom">
                <div class="container">
                    <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#main-nav-container" aria-expanded="false">
                        <span class="toggle-text">Menü</span>
                        <span class="toggle-wrapper">
                            <span class="sr-only">Toggle navigation</span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </span>
                    </button>
                    <div class="collapse navbar-collapse" id="main-nav-container">
                        {{ Model.Navigation | Display }}
                    </div>
                </div>
            </div>
        </header>
    </div>
    <div class="main">
        <div class="page-header larger parallax custom">
            <div class="container">
                <h1>Philip Hunter</h1>
            </div>
        </div>

        {% if Model.Messages != null %}
            <div class="container">
                <div class="row">
                    <div class="col-md-9">
                        {{ Model.Messages | Display }}
                    </div>
                </div>
            </div>
        {% endif %}

        <div class="container">
            <div class="row">
                {% if Model.Content != null %}
                    {{ Model.Content | Display }}
                {% endif %}

                {% if Model.AsideFirst != null %}
                    <div class="col-md-3 col-md-pull-6 sidebar">
                        {{ Model.AsideFirst | Display }}
                    </div>
                {% endif %}
                
                {% if Model.AsideSecond != null %}
                    <aside class="col-md-3 sidebar">
                        {{ Model.AsideSecond | Display }}
                    </aside>
                {% endif %}
            </div>
        </div>
    </div>
    <div id="footer-wrapper">
        <footer>
            {{ Model.Footer | Display }}
        </footer>
    </div>
</div>